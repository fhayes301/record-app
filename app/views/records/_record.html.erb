<tr>
  <td></td>
  <td><%= record.genre %></td>
  <td><%= record.artist %></td>
  <td><%= record.album %></td>
  <td><%= record.year %></td>
  <td><%= record.quantity %></td>
  <td><%= link_to "Show", record_path(record) %> </td>
  <td><%= link_to "Edit", edit_record_path(record) %></td>
  <td><%= link_to "Delete", record_path(record),
          method: :delete,
          data: { confirm: 'Are you sure?' } %> </td>
</tr>
